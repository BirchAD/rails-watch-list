<header class="banner">
  <h1 class> My Lists: </h1>
</header>
<div class="container">
  <div class="row">
    <% @lists.each do |list| %>
      <div class="col-auto d-flex">
        <div class="card" style="width: 18rem;">
          <div class="card-body">
            <img class="card-img-top" src="<%= cl_image_path list.photo.key%>" alt="movie image">
              <h5 class="card-title mt-2">
                <%= link_to list.name, list_path(list), class: "btn btn-primary" %>
              </h5>
              <%= link_to "Delete",
              list_path(list),
              data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}, class: "btn btn-primary"%>
          </div>
        </div>
      </div>
        <% end %>
  </div>
</div>
